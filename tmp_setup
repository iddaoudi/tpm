source launch_script qr 16384 1024 ${numa} ${numa} ${machine} mid
source launch_script qr 16384 1024 ${threads} ${numa} ${machine} mid
source launch_script qr 16384 1024 ${numa} ${numa} ${machine} min
source launch_script qr 16384 1024 ${threads} ${numa} ${machine} min
