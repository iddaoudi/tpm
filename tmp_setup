#!/bin/bash

machine=skylake
threads=48
numa=24

export LD_LIBRARY_PATH=$LD_LIBRARY_PATH:/home/cc/OpenBLAS/build/lib

#source launch_script qr 16384 256 ${numa} ${numa} ${machine} mid
#source launch_script qr 16384 256 ${threads} ${numa} ${machine} mid
#source launch_script qr 16384 256 ${numa} ${numa} ${machine} min
#source launch_script qr 16384 256 ${threads} ${numa} ${machine} min
echo ">>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>> Exp qr 1 OK"
#source launch_script qr 16384 512 ${numa} ${numa} ${machine} mid
source launch_script qr 16384 512 ${threads} ${numa} ${machine} mid
source launch_script qr 16384 512 ${numa} ${numa} ${machine} min
source launch_script qr 16384 512 ${threads} ${numa} ${machine} min
