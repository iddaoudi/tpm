source launch_script sparselu 128 64 ${numa} ${numa} ${machine} mid
source launch_script sparselu 128 64 ${threads} ${numa} ${machine} mid
source launch_script sparselu 128 64 ${numa} ${numa} ${machine} min
source launch_script sparselu 128 64 ${threads} ${numa} ${machine} min
